{
  "compilerOptions": {
    "noImplicitAny": false,
    "outDir": "./dist/",
    "baseUrl": "./",
    "paths": {
      "@constants/*": ["./cypress/constants/*"], 
      "@pages/*": ["./cypress/Pages/*"], 
      "@helpers/*": ["./cypress/helpers/*"]
    },
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "types": [
      "cypress"
  ],
    "moduleResolution": "node",
    "strictNullChecks":false,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts"
  ]
}
